{
  "name": "@tityus/vue-uploader",
  "version": "0.5.1",
  "private": false,
  "description": "Vue uploader component.",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-uploader ./src/index.ts",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/vue-uploader.common.js",
  "unpkg": "dist/vue-uploader.umd.min.js",
  "dependencies": {
    "vuetify": "^2.3.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-service": "^4.1.2",
    "core-js": "^3.1.3",
    "lint-staged": "^8.1.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "typescript": "^3.4.3",
    "vue": "^2.6.10",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  },
  "bugs": {
    "url": "https://github.com/tityus/vue-uploader/issues"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://github.com/tityus/vue-uploader#readme",
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
